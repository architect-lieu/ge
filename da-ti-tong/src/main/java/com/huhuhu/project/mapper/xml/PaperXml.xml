<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huhuhu.project.mapper.PaperMapper">

    <select id="statisticPaperQuestionNum" resultType="java.util.Map">
        select paper_id as paperId,count(1) as num from dtt_paper_question where paper_id in
        <foreach collection="paperIds" item="pId" separator="," open="(" close=")">
            #{pId}
        </foreach>
        group by paper_id
    </select>
</mapper>
